local Tab = Window:Tab({
    Title = "mine",
    Icon = "pickaxe", -- optional
    Locked = false,
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")

local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

local MineFolder = workspace:WaitForChild("MineBlocks")

local mining = false
local holding = false
local mode = "None"

local currentESP = nil
local camConnection = nil

local function holdClick(state)
    if state and not holding then
        holding = true
        VirtualInputManager:SendMouseButtonEvent(0,0,0,true,game,0)
    elseif not state and holding then
        holding = false
        VirtualInputManager:SendMouseButtonEvent(0,0,0,false,game,0)
    end
end

local function teleportTo(cf)
    local char = LocalPlayer.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.CFrame = cf + Vector3.new(0,3,0)
    end
end

local function getPart(model)
    return model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart", true)
end

local function setESP(part)
    if currentESP then
        currentESP:Destroy()
        currentESP = nil
    end

    local box = Instance.new("BoxHandleAdornment")
    box.Adornee = part
    box.AlwaysOnTop = true
    box.ZIndex = 10
    box.Size = part.Size + Vector3.new(0.3,0.3,0.3)
    box.Color3 = Color3.fromRGB(255, 0, 0)
    box.Transparency = 0.5
    box.Parent = part

    currentESP = box
end

local function lockCamera(part)
    if camConnection then camConnection:Disconnect() end

    camConnection = RunService.RenderStepped:Connect(function()
        if not mining or not part or not part.Parent then return end
        Camera.CFrame = CFrame.new(Camera.CFrame.Position, part.Position)
    end)
end

Tab:Dropdown({
    Title = "Auto Mode",
    Values = {"None", "With Auto Lock"},
    Value = "None",
    Callback = function(v)
        mode = v
    end
})

Tab:Toggle({
    Title = "Auto Mine (Read notes)",
    Desc = "Teleport to block",
    Value = false,
    Callback = function(state)
        mining = state

        if not state then
            holdClick(false)
            if camConnection then camConnection:Disconnect() camConnection = nil end
            if currentESP then currentESP:Destroy() currentESP = nil end
            return
        end

        task.spawn(function()
            while mining do
                for _, model in ipairs(MineFolder:GetChildren()) do
                    if not mining then break end

                    local part = getPart(model)
                    if part then
                        teleportTo(part.CFrame)

                        if mode == "With Auto Lock + ESP" then
                            setESP(part)
                            lockCamera(part)
                        end

                        holdClick(true)
                        task.wait(2.5) 
                    end
                end
            end
        end)
    end
})

local Section = Tab:Section({ 
    Title = "note Auto Mine only work if you hold pickaxe",
})

local Players = game:GetService("Players")
local ProximityPromptService = game:GetService("ProximityPromptService")
local LocalPlayer = Players.LocalPlayer

local eggFolder = workspace:WaitForChild("EggsToGrab")
local enabled = false

local function getBasePosition()
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BillboardGui") or v:IsA("SurfaceGui") then
            local label = v:FindFirstChildWhichIsA("TextLabel", true)
            if label and label.Text:lower() == "your base" then
                local adornee = v.Adornee
                if adornee and adornee:IsA("BasePart") then
                    return adornee.CFrame + Vector3.new(0,3,0)
                end
            end
        end
    end
end

local function teleport(cf)
    local char = LocalPlayer.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.CFrame = cf
    end
end

Tab:Toggle({
    Title = "Auto teleport base",
    Desc = "pick up egg auto teleport base",
    Value = false,
    Callback = function(state)
        enabled = state
        if not state then return end

        task.spawn(function()
            while enabled do
                for _, model in pairs(eggFolder:GetChildren()) do
                    if not enabled then break end

                    for _, v in pairs(model:GetDescendants()) do
                        if v:IsA("ProximityPrompt") then
                            local part = v.Parent
                            if part and part:IsA("BasePart") then
                                teleport(part.CFrame + Vector3.new(0,3,0))
                                task.wait(0.5)

                                fireproximityprompt(v)
                                task.wait(0.7)

                                local baseCF = getBasePosition()
                                if baseCF then
                                    teleport(baseCF)
                                end

                                task.wait(1)
                            end
                        end
                    end
                end
                task.wait(1)
            end
        end)
    end
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

local XRAY_ENABLED = false
local XRAY_DISTANCE = 25
local XRAY_TRANSPARENCY = 0.6

local originalTransparency = {}

local function setCharacter()
    Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
end

LocalPlayer.CharacterAdded:Connect(setCharacter)

local function isNearPlayer(part)
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return false
    end
    local hrp = Character.HumanoidRootPart
    return (part.Position - hrp.Position).Magnitude <= XRAY_DISTANCE
end

RunService.RenderStepped:Connect(function()
    if not XRAY_ENABLED then return end
    if not Character then return end

    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and not obj:IsDescendantOf(Character) then
            if isNearPlayer(obj) then
                if originalTransparency[obj] == nil then
                    originalTransparency[obj] = obj.Transparency
                end
                obj.Transparency = XRAY_TRANSPARENCY
            else
                if originalTransparency[obj] ~= nil then
                    obj.Transparency = originalTransparency[obj]
                    originalTransparency[obj] = nil
                end
            end
        end
    end
end)

Tab:Toggle({
    Title = "x-ray Near Player",
    Desc = "no lag",
    Default = false,
    Callback = function(state)
        XRAY_ENABLED = state

        if not state then
            for part, transparency in pairs(originalTransparency) do
                if part and part.Parent then
                    part.Transparency = transparency
                end
            end
            table.clear(originalTransparency)
        end
    end
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")

local SellAll = false

local MerchantPosition = Vector3.new(0, 0, 0) 

local function clickCenter()
    local cam = workspace.CurrentCamera.ViewportSize
    VirtualInputManager:SendMouseButtonEvent(
        cam.X/2, cam.Y/2, 0, true, game, 0
    )
    task.wait(0.05)
    VirtualInputManager:SendMouseButtonEvent(
        cam.X/2, cam.Y/2, 0, false, game, 0
    )
end

local function getNearestPrompt()
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return nil end

    local nearest, dist = nil, math.huge
    for _,v in pairs(workspace:GetDescendants()) do
        if v:IsA("ProximityPrompt") and v.Enabled then
            local part = v.Parent
            if part:IsA("BasePart") then
                local d = (hrp.Position - part.Position).Magnitude
                if d < dist and d < 20 then
                    dist = d
                    nearest = v
                end
            end
        end
    end
    return nearest
end

task.spawn(function()
    while true do
        task.wait(1)
        if SellAll then
            local prompt = getNearestPrompt()
            if prompt then
                fireproximityprompt(prompt)
            end

            for _,gui in pairs(LocalPlayer.PlayerGui:GetDescendants()) do
                if gui:IsA("TextButton") or gui:IsA("TextLabel") then
                    local txt = gui.Text:lower()
                    if txt:find("sell") or txt:find("inventory") then
                        pcall(function()
                            gui:Activate()
                        end)
                    end
                end
            end

            clickCenter()
        end
    end
end)

local Toggle = Tab:Toggle({
    Title = "Auto Sell All",
    Desc = "Auto sell resource",
    Icon = "shopping-cart",
    Value = false,
    Callback = function(state)
        SellAll = state
    end
})

local Tab = Window:Tab({
    Title = "Egg",
    Icon = "egg", -- optional
    Locked = false,
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local folder = workspace:WaitForChild("EggsToGrab")

local espEnabled = false
local espObjects = {} 

local function clearESP()
    for _, esp in pairs(espObjects) do
        if esp and esp.Parent then
            esp:Destroy()
        end
    end
    table.clear(espObjects)
end

local function createESP(model)
    if not model:IsA("Model") then return end

    local part = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart", true)
    if not part then return end

    local box = Instance.new("BoxHandleAdornment")
    box.Name = "ESP_EGG"
    box.Adornee = part
    box.AlwaysOnTop = true
    box.ZIndex = 10
    box.Size = part.Size + Vector3.new(0.2, 0.2, 0.2)
    box.Transparency = 0.5
    box.Color3 = Color3.fromRGB(0, 255, 0)
    box.Parent = part

    table.insert(espObjects, box)
end

Tab:Toggle({
    Title = "ESP Egg",
    Desc = "ESP box",
    Value = false,
    Callback = function(state)
        espEnabled = state

        if not state then
            clearESP()
            return
        end

        task.spawn(function()
            while espEnabled do
                clearESP() -- prevent duplicate esp

                for _, model in ipairs(folder:GetChildren()) do
                    if not espEnabled then break end
                    createESP(model)
                end

                task.wait(1)
            end
        end)
    end
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local eggFolder = workspace:WaitForChild("EggsToGrab")
local toggleOn = false

local function teleportTo(part)
    local char = LocalPlayer.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.CFrame = part.CFrame + Vector3.new(0, 3, 0)
    end
end

Tab:Toggle({
    Title = "auto pick up egg",
    Desc = "Teleport egg",
    Value = false,
    Callback = function(state)
        toggleOn = state

        if not state then return end

        task.spawn(function()
            while toggleOn do
                for _, model in ipairs(eggFolder:GetChildren()) do
                    if not toggleOn then break end
                    -- Cari prompt di model
                    local prompt = model:FindFirstChildOfClass("ProximityPrompt", true)
                    local part = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart", true)
                    if prompt and part then

                        teleportTo(part)
                        task.wait(0.3)

                        fireproximityprompt(prompt)
                        task.wait(2) 
                    end
                end
            end
        end)
    end
})

