local Tab = Window:Tab({
    Title = "Jewel boxes",
    Icon = "box", -- optional
    Locked = false,
})

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local Char = LP.Character or LP.CharacterAdded:Wait()
local HRP = Char:WaitForChild("HumanoidRootPart")

local AutoJewel = false
local DelayTime = 2

local function getMyBase()
    if not workspace:FindFirstChild("Bases") then return nil end

    local bases = workspace.Bases
    if bases:FindFirstChild("5") then
        return bases["5"]
    end

    for _,base in pairs(bases:GetChildren()) do
        if base:FindFirstChild("GrabPad") and base:FindFirstChild("CollectPad") then
            return base
        end
    end

    return nil
end

local function getPromptFromModel(model)
    for _,v in pairs(model:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            return v
        end
    end
end

local function tpToModel(model)
    local part = model:FindFirstChildWhichIsA("BasePart", true)
    if part then
        HRP.CFrame = part.CFrame + Vector3.new(0, 3, 0)
    end
end

task.spawn(function()
    while true do
        task.wait(0.2)
        if AutoJewel then
            local base = getMyBase()
            if not base then
                warn("Base not found")
                task.wait(1)
                continue
            end

            local grabPad = base:FindFirstChild("GrabPad")
            if grabPad then
                local grabPrompt = getPromptFromModel(grabPad)
                if grabPrompt then
                    tpToModel(grabPad)
                    task.wait(0.3)
                    fireproximityprompt(grabPrompt)
                end
            end

            task.wait(DelayTime)

            local collectPad = base:FindFirstChild("CollectPad")
            if collectPad then
                local sellPrompt = getPromptFromModel(collectPad)
                if sellPrompt then
                    tpToModel(collectPad)
                    task.wait(0.3)
                    fireproximityprompt(sellPrompt)
                end
            end

            task.wait(DelayTime)
        end
    end
end)

Tab:Dropdown({
    Title = "Jewel Delay",
    Desc = "Delay Auto Jewel",
    Values = {"2 Delay", "7 Delay", "10 Delay"},
    Value = "2 Delay",
    Callback = function(opt)
        if opt == "2 Delay" then
            DelayTime = 2
        elseif opt == "7 Delay" then
            DelayTime = 7
        elseif opt == "10 Delay" then
            DelayTime = 10
        end
    end
})

Tab:Toggle({
    Title = "Auto Jewel Boxes",
    Desc = "Auto Grab Sell Jewel Boxes",
    Icon = "gem",
    Value = false,
    Callback = function(state)
        AutoJewel = state
    end
})

